<?xml version="1.0" encoding="UTF-8"?>
<services-config>
  <services>
    <service id="amfphp-flashremoting-service"
        class="flex.messaging.services.RemotingService"
        messageTypes="flex.messaging.messages.RemotingMessage">
      
      <destination id="SucursalesService">
        <channels>
          <channel ref="my-amf"/>
        </channels>
        <properties>
          <source>Weborb.cau.SucursalesService</source>
        </properties>
      </destination>
      <destination id="ClientesService">
        <channels>
          <channel ref="my-amf"/>
        </channels>
        <properties>
          <source>Weborb.cau.ClientesService</source>
        </properties>
      </destination>
      <destination id="MantenedorService">
        <channels>
          <channel ref="my-amf"/>
        </channels>
        <properties>
          <source>Weborb.cau.MantenedorService</source>
        </properties>
      </destination>
      
      <destination id="UsuariosService">
        <channels>
          <channel ref="my-amf"/>
        </channels>
        <properties>
          <source>Weborb.cau.UsuariosService</source>
        </properties>
      </destination>
      <destination id="GenericDestination">
        <channels>
          <channel ref="my-amf"/>
        </channels>
        <properties>
          <source>*</source>
        </properties>
      </destination>
    </service>  
    
  </services>

  <channels>
    <channel-definition id="my-amf" class="mx.messaging.channels.AMFChannel">
      <endpoint uri="http://localhost:8080/webOrb/Weborb/index.php" class="flex.messaging.endpoints.AMFEndpoint"/>
    </channel-definition>
    
  </channels>

</services-config>