<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%" xmlns:components="components.*">
	
	<fx:Script>
		<![CDATA[
			import mx.core.FlexGlobals;
			protected function fnVolcadoDatos(event:MouseEvent):void
			{
				// TODO Auto-generated method stub
				CAUWeb(FlexGlobals.topLevelApplication).vsGeneral.selectedIndex = 9;
				CAUWeb(FlexGlobals.topLevelApplication).titulos.strBreadcrumb = [{label: 'Home', data: CAUWeb(FlexGlobals.topLevelApplication).vsGeneral.getElementAt(0)}, {label: 'Reportes', data: CAUWeb(FlexGlobals.topLevelApplication).vsGeneral.getElementAt(8)}, {label: 'Volcado de Datos', data: CAUWeb(FlexGlobals.topLevelApplication).vsGeneral.getElementAt(5)}];
			}
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<components:CustomPanelSpark title="Listado de Informes Centro de atencion a Usuarios" width="600" height="600">
		<mx:LinkButton label="Volcado de Datos" click="fnVolcadoDatos(event)"/>
	</components:CustomPanelSpark>
</s:Group>
